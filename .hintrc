{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "width: fit-content",
          "-ms-touch-action"
        ]
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "axe/parsing": [
      "default",
      {
        "duplicate-id-aria": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not ios_saf <= 16.5",
    "not safari <= 16.5"
  ]
}